<h1>Listing members</h1>

<table>
  <tr>
    <th>User name</th>
    <th>First name</th>
    <th>Last name</th>
    <th>Password</th>
    <th>Street number</th>
    <th>Street name</th>
    <th>City</th>
    <th>State</th>
    <th>Zip code</th>
    <th>Email address</th>
    <th>Religon</th>
    <th>Education level</th>
    <th>Partner</th>
    <th>Employment status</th>
    <th>Age</th>
    <th>Number of children</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @members.each do |member| %>
  <tr>
    <td><%= member.user_name %></td>
    <td><%= member.first_name %></td>
    <td><%= member.last_name %></td>
    <td><%= member.password %></td>
    <td><%= member.street_number %></td>
    <td><%= member.street_name %></td>
    <td><%= member.city %></td>
    <td><%= member.state %></td>
    <td><%= member.zip_code %></td>
    <td><%= member.email_address %></td>
    <td><%= member.religon %></td>
    <td><%= member.education_level %></td>
    <td><%= member.partner %></td>
    <td><%= member.employment_status %></td>
    <td><%= member.age %></td>
    <td><%= member.number_of_children %></td>
    <td><%= link_to 'Show', member %></td>
    <td><%= link_to 'Edit', edit_member_path(member) %></td>
    <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Member', new_member_path %>
