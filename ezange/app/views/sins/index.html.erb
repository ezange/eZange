<h1>Listing sins</h1>

<table>
  <tr>
    <th>Member</th>
    <th>Title</th>
    <th>Post date</th>
    <th>Post time</th>
    <th>Sin date</th>
    <th>Sin time</th>
    <th>Sin lat</th>
    <th>Sin lot</th>
    <th>Sin</th>
    <th>Sin street number</th>
    <th>Sin street name</th>
    <th>Sin city</th>
    <th>Sin state</th>
    <th>Sin zip</th>
    <th>Sin country</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sins.each do |sin| %>
  <tr>
    <td><%= sin.member_id %></td>
    <td><%= sin.title %></td>
    <td><%= sin.post_date %></td>
    <td><%= sin.post_time %></td>
    <td><%= sin.sin_date %></td>
    <td><%= sin.sin_time %></td>
    <td><%= sin.sin_lat %></td>
    <td><%= sin.sin_lot %></td>
    <td><%= sin.sin %></td>
    <td><%= sin.sin_street_number %></td>
    <td><%= sin.sin_street_name %></td>
    <td><%= sin.sin_city %></td>
    <td><%= sin.sin_state %></td>
    <td><%= sin.sin_zip %></td>
    <td><%= sin.sin_country %></td>
    <td><%= link_to 'Show', sin %></td>
    <td><%= link_to 'Edit', edit_sin_path(sin) %></td>
    <td><%= link_to 'Destroy', sin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Sin', new_sin_path %>
